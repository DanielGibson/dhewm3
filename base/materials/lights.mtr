		forceHighQuality
		map lights/square_light_simple.tga
		colored
		zeroClamp
	}
}

lights/defaultProjectedLight
{
	lightFalloffImage	_noFalloff
	{
		forceHighQuality
		map lights/square_light_simple.tga
		colored
		zeroClamp
	}
}

